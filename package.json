{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "description": "read & write named.conf",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "engines": {
    "node": ">=8.9.3"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "dependencies": {
    "pratt-parser": "^7.0.9"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "rollup": "^0.53.0",
    "rollup-plugin-babel": "^3.0.3",
    "semantic-release": "^11.0.2",
    "xo": "^0.19.0",
    "documentation": "^5.3.5"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "docs": "documentation readme src/parser.js --section=API",
    "test": "ava",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/parser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/named-conf.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
