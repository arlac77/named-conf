{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "description": "read & write named.conf",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/parser.js --section=API",
    "lint": "documentation lint src/parser.js"
  },
  "dependencies": {
    "pratt-parser": "^7.0.9"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.7.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.3.0",
    "rollup": "^2.6.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.3",
    "semantic-release": "^17.0.6"
  },
  "engines": {
    "node": ">=12.16.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/named-conf.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
