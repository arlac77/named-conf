{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "description": "read & write named.conf",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "engines": {
    "node": ">=7.10.0"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "dependencies": {
    "pratt-parser": "^7.0.3"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.3.0",
    "rollup": "^0.41.6",
    "semantic-release": "^6.3.6",
    "ava": "^0.19.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "jsdoc-babel": "^0.3.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1"
  },
  "scripts": {
    "cover": "nyc ava",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/parser.js -c -- src/parser.js",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "markdown-doctest",
    "precover": "rollup -c rollup.config.test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/named-conf.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}