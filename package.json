{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "description": "read & write named.conf",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "engines": {
    "node": ">=6.9.2"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "dependencies": {
    "pratt-parser": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "2.11.15",
    "istanbul": "^0.4.5",
    "markdown-doctest": "^0.9.1",
    "jsdoc-to-markdown": "^2.0.1",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "rollup": "^0.41.1",
    "semantic-release": "^6.3.5",
    "cz-conventional-changelog": "^1.2.0"
  },
  "scripts": {
    "cover": "npm run prepublish && istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/parser.js -c -- src/parser.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/named-conf.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}