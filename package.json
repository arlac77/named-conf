{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/parser.mjs",
  "exports": {
    ".": "src/parser.mjs"
  },
  "description": "read & write named.conf",
  "keywords": [
    "bind",
    "named.conf"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/parser.mjs --section=API",
    "lint": "documentation lint src/parser.mjs"
  },
  "dependencies": {
    "pratt-parser": "^8.0.7"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "c8": "^7.2.0",
    "documentation": "^13.0.1",
    "semantic-release": "^17.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/named-conf.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-arlac77-github"
    ]
  }
}
