{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "description": "read & write named.conf",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "dependencies": {
    "pratt-parser": "^7.0.6"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.1.0",
    "rollup": "^0.47.4",
    "semantic-release": "^6.3.6",
    "ava": "^0.22.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "jsdoc-babel": "^0.3.0",
    "xo": "^0.19.0",
    "babel-preset-stage-3": "^6.24.1"
  },
  "scripts": {
    "cover": "nyc ava",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c -- src/parser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/named-conf.git"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
