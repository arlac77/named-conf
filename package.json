{
  "name": "named-conf",
  "version": "0.0.0-semantic-release",
  "description": "read & write named.conf",
  "main": "dist/parser.js",
  "module": "src/parser.js",
  "keywords": [
    "named.conf",
    "bind"
  ],
  "engines": {
    "node": ">=6.9.1"
  },
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/named-conf/issues"
  },
  "homepage": "https://github.com/arlac77/named-conf#readme",
  "dependencies": {
    "pratt-parser": "^2.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.14",
    "istanbul": "^0.4.5",
    "markdown-doctest": "^0.8.1",
    "jsdoc-to-markdown": "^2.0.1",
    "mocha": "3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "rollup": "^0.36.3",
    "semantic-release": "6.3.1",
    "cz-conventional-changelog": "1.2.0"
  },
  "scripts": {
    "cover": "./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/parser.js -- src/parser.js",
    "docs": "jsdoc2md -l off -t README.hbs -f src/parser.js > README.md",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "doc": "jsdoc lib/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/named-conf.git"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}